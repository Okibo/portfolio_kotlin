<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.portfolio.kotlinapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KotlinApp">
        <activity
            android:name=".nasafeed.NASAFeedActivity"
            android:parentActivityName=".mainmenu.MainActivity"/>
        <activity
            android:name=".todo.ToDoActivity"
            android:parentActivityName=".mainmenu.MainActivity"/>
        <activity
            android:name=".tictactoe.TicTacToeMenuActivity"
            android:parentActivityName=".mainmenu.MainActivity"/>
        <activity
            android:name=".tictactoe.TicTacToeActivity"
            android:parentActivityName=".tictactoe.TicTacToeMenuActivity" />
        <activity android:name=".mainmenu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>